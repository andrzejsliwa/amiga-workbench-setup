; Arcade Game Selector 2 Demos Startup Script
; -------------------------------------------


; End script, if ags2 demos path is not defined
IF NOT EXISTS ENV:ags2demospath
	SKIP end
ENDIF


; Assign AGS
Assign AGS: $ags2demospath


; Copy whdload args to env, if it exists. Otherwise use default
IF EXISTS AGS:whdloadargs
  copy >NIL: AGS:whdloadargs ENV:whdloadargs
ELSE
  setenv whdloadargs ""
ENDIF


; Set menu path to ags2 demos path
setenv menupath "$ags2demospath"


; Start AGS2
IF EXISTS AGS:AGS2
  AGS:AGS2
ENDIF


; Remove AGS assign
ASSIGN AGS: $ags2demospath REMOVE


; End
LAB end